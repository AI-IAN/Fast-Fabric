 "cells": [
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Fabric Fast-Track Data Quality Validation\n",
    "\n",
    "This notebook implements Great Expectations data quality checks for the Fast-Track accelerator.\n",
    "It validates data quality across bronze, silver, and gold layers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Install and import required libraries\n",
    "import great_expectations as gx\n",
    "import pandas as pd\n",
    "import json\n",
    "import os\n",
    "from datetime import datetime\n",
    "from pathlib import Path\n",
    "\n",
    "# Fabric SDK imports\n",
    "from microsoft.fabric.core import FabricRestClient\n",
    "from microsoft.fabric.lakehouse import LakehouseClient"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Configuration\n",
    "WORKSPACE_NAME = os.getenv('WORKSPACE_NAME', 'FastTrack-Test-Workspace')\n",
    "LAKEHOUSE_NAME = 'FastTrack_Lakehouse'\n",
    "\n",
    "# Data quality thresholds\n",
    "MIN_ROWS_BRONZE = 1000\n",
    "MAX_NULL_PERCENTAGE = 0.05  # 5%\n",
    "MIN_UNIQUE_CUSTOMERS = 100\n",
    "\n",
    "# Initialize Fabric clients\n",
    "fabric_client = FabricRestClient()\n",
    "lakehouse_client = LakehouseClient()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Initialize Great Expectations context\n",
    "context = gx.get_context()\n",
    "\n",
    "# Create data source for Fabric Lakehouse\n",
    "data_source_config = {\n",
    "    \"name\": \"fabric_lakehouse_datasource\",\n",
    "    \"class_name\": \"Datasource\",\n",
    "    \"module_name\": \"great_expectations.datasource\",\n",
    "    \"execution_engine\": {\n",
    "        \"class_name\": \"SparkDFExecutionEngine\",\n",
    "        \"module_name\": \"great_expectations.execution_engine\"\n",
    "    },\n",
    "    \"data_connectors\": {\n",
    "        \"default_runtime_data_connector\": {\n",
    "            \"class_name\": \"RuntimeDataConnector\",\n",
    "            \"module_name\": \"great_expectations.datasource.data_connector\",\n",
    "            \"batch_identifiers\": [\"default_identifier_name\"]\n",
    "        }\n",
    "    }\n",
    "}\n",
    "\n",
    "context.add_datasource(**data_source_config)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Bronze Layer Data Quality Checks"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def validate_bronze_layer():\n",
    "    \"\"\"Validate bronze layer data quality\"\"\"\n",
    "    results = []\n",
    "    \n",
    "    # Read bronze tables\n",
    "    customers_df = spark.read.table(f\"{LAKEHOUSE_NAME}.bronze_customers\")\n",
    "    orders_df = spark.read.table(f\"{LAKEHOUSE_NAME}.bronze_orders\")\n",
    "    products_df = spark.read.table(f\"{LAKEHOUSE_NAME}.bronze_products\")\n",
    "    \n",
    "    # Create expectations suite\n",
    "    suite = context.create_expectation_suite(\n",
    "        expectation_suite_name=\"bronze_layer_suite\",\n",
    "        overwrite_existing=True\n",
    "    )\n",
    "    \n",
    "    # Customer table validations\n",
    "    batch_request = {\n",
    "        \"datasource_name\": \"fabric_lakehouse_datasource\",\n",
    "        \"data_connector_name\": \"default_runtime_data_connector\",\n",
    "        \"data_asset_name\": \"customers\",\n",
    "        \"runtime_parameters\": {\"batch_data\": customers_df},\n",
    "        \"batch_identifiers\": {\"default_identifier_name\": \"customers\"}\n",
    "    }\n",
    "    \n",
    "    validator = context.get_validator(\n",
    "        batch_request=batch_request,\n",
    "        expectation_suite_name=\"bronze_layer_suite\"\n",
    "    )\n",
    "    \n",
    "    # Add expectations\n",
    "    validator.expect_table_row_count_to_be_between(min_value=MIN_ROWS_BRONZE)\n",
    "    validator.expect_column_to_exist(\"customer_id\")\n",
    "    validator.expect_column_values_to_not_be_null(\"customer_id\")\n",
    "    validator.expect_column_values_to_be_unique(\"customer_id\")\n",
    "    validator.expect_column_to_exist(\"email\")\n",
    "    validator.expect_column_values_to_match_regex(\"email\", r\"^[\\w\\.-]+@[\\w\\.-]+\\.[a-zA-Z]{2,}$\")\n",
    "    \n",
    "    # Run validation\n",
    "    validation_result = validator.validate()\n",
    "    results.append({\"table\": \"bronze_customers\", \"result\": validation_result})\n",
    "    \n",
    "    return results"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Silver Layer Data Quality Checks"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def validate_silver_layer():\n",
    "    \"\"\"Validate silver layer data quality\"\"\"\n",
    "    results = []\n",
    "    \n",
    "    # Read silver tables\n",
    "    dim_customer_df = spark.read.table(f\"{LAKEHOUSE_NAME}.dim_customer\")\n",
    "    fact_sales_df = spark.read.table(f\"{LAKEHOUSE_NAME}.fact_sales\")\n",
    "    \n",
    "    # Create expectations suite\n",
    "    suite = context.create_expectation_suite(\n",
    "        expectation_suite_name=\"silver_layer_suite\",\n",
    "        overwrite_existing=True\n",
    "    )\n",
    "    \n",
    "    # Dimension table validations\n",
    "    batch_request = {\n",
    "        \"datasource_name\": \"fabric_lakehouse_datasource\",\n",
    "        \"data_connector_name\": \"default_runtime_data_connector\",\n",
    "        \"data_asset_name\": \"dim_customer\",\n",
    "        \"runtime_parameters\": {\"batch_data\": dim_customer_df},\n",
    "        \"batch_identifiers\": {\"default_identifier_name\": \"dim_customer\"}\n",
    "    }\n",
    "    \n",
    "    validator = context.get_validator(\n",
    "        batch_request=batch_request,\n",
    "        expectation_suite_name=\"silver_layer_suite\"\n",
    "    )\n",
    "    \n",
    "    # Add expectations for dimension table\n",
    "    validator.expect_column_values_to_not_be_null(\"customer_key\")\n",
    "    validator.expect_column_values_to_be_unique(\"customer_key\")\n",
    "    validator.expect_column_values_to_not_be_null(\"customer_name\")\n",
    "    validator.expect_column_proportion_of_unique_values_to_be_between(\"customer_name\", min_value=0.8)\n",
    "    \n",
    "    # Validate referential integrity\n",
    "    validator.expect_column_values_to_be_in_set(\"is_active\", [0, 1, True, False])\n",
    "    \n",
    "    validation_result = validator.validate()\n",
    "    results.append({\"table\": \"dim_customer\", \"result\": validation_result})\n",
    "    \n",
    "    return results"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Gold Layer Data Quality Checks"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def validate_gold_layer():\n",
    "    \"\"\"Validate gold layer aggregations and business rules\"\"\"\n",
    "    results = []\n",
    "    \n",
    "    # Read gold tables\n",
    "    monthly_sales_df = spark.read.table(f\"{LAKEHOUSE_NAME}.gold_monthly_sales\")\n",
    "    customer_metrics_df = spark.read.table(f\"{LAKEHOUSE_NAME}.gold_customer_metrics\")\n",
    "    \n",
    "    # Create expectations suite\n",
    "    suite = context.create_expectation_suite(\n",
    "        expectation_suite_name=\"gold_layer_suite\",\n",
    "        overwrite_existing=True\n",
    "    )\n",
    "    \n",
    "    # Monthly sales validations\n",
    "    batch_request = {\n",
    "        \"datasource_name\": \"fabric_lakehouse_datasource\",\n",
    "        \"data_connector_name\": \"default_runtime_data_connector\",\n",
    "        \"data_asset_name\": \"monthly_sales\",\n",
    "        \"runtime_parameters\": {\"batch_data\": monthly_sales_df},\n",
    "        \"batch_identifiers\": {\"default_identifier_name\": \"monthly_sales\"}\n",
    "    }\n",
    "    \n",
    "    validator = context.get_validator(\n",
    "        batch_request=batch_request,\n",
    "        expectation_suite_name=\"gold_layer_suite\"\n",
    "    )\n",
    "    \n",
    "    # Business rule validations\n",
    "    validator.expect_column_values_to_be_between(\"total_sales\", min_value=0)\n",
    "    validator.expect_column_values_to_be_between(\"total_orders\", min_value=0)\n",
    "    validator.expect_column_sum_to_be_between(\"total_sales\", min_value=10000)  # Minimum expected revenue\n",
    "    \n",
    "    # Date validations\n",
    "    validator.expect_column_values_to_be_dateutil_parseable(\"month_year\")\n",
    "    \n",
    "    validation_result = validator.validate()\n",
    "    results.append({\"table\": \"gold_monthly_sales\", \"result\": validation_result})\n",
    "    \n",
    "    return results"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Execute All Data Quality Checks"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def run_all_data_quality_checks():\n",
    "    \"\"\"Execute all data quality validations\"\"\"\n",
    "    print(f\"Starting data quality validation at {datetime.now()}\")\n",
    "    \n",
    "    all_results = []\n",
    "    \n",
    "    try:\n",
    "        # Run bronze layer checks\n",
    "        print(\"Validating Bronze Layer...\")\n",
    "        bronze_results = validate_bronze_layer()\n",
    "        all_results.extend(bronze_results)\n",
    "        \n",
    "        # Run silver layer checks\n",
    "        print(\"Validating Silver Layer...\")\n",
    "        silver_results = validate_silver_layer()\n",
    "        all_results.extend(silver_results)\n",
    "        \n",
    "        # Run gold layer checks\n",
    "        print(\"Validating Gold Layer...\")\n",
    "        gold_results = validate_gold_layer()\n",
    "        all_results.extend(gold_results)\n",
    "        \n",
    "        # Generate summary report\n",
    "        generate_summary_report(all_results)\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"Error during data quality validation: {str(e)}\")\n",
    "        raise\n",
    "    \n",
    "    return all_results"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def generate_summary_report(results):\n",
    "    \"\"\"Generate data quality summary report\"\"\"\n",
    "    total_checks = 0\n",
    "    passed_checks = 0\n",
    "    failed_checks = 0\n",
    "    \n",
    "    print(\"\\n=== DATA QUALITY SUMMARY REPORT ===")\n",
    "    print(f\"Generated: {datetime.now()}\")\n",
    "    print(f\"Workspace: {WORKSPACE_NAME}\")\n",
    "    print(f\"Lakehouse: {LAKEHOUSE_NAME}\")\n",
    "    print(\"\\n--- Results by Table ---\")\n",
    "    \n",
    "    for result in results:\n",
    "        table_name = result['table']\n",
    "        validation_result = result['result']\n",
    "        \n",
    "        table_total = len(validation_result.results)\n",
    "        table_passed = sum(1 for r in validation_result.results if r.success)\n",
    "        table_failed = table_total - table_passed\n",
    "        \n",
    "        total_checks += table_total\n",
    "        passed_checks += table_passed\n",
    "        failed_checks += table_failed\n",
    "        \n",
    "        status = \"✅ PASS\" if table_failed == 0 else \"❌ FAIL\"\n",
    "        print(f\"{table_name}: {status} ({table_passed}/{table_total} checks passed)\")\n",
    "        \n",
    "        # Show failed expectations\n",
    "        if table_failed > 0:\n",
    "            print(f\"  Failed expectations for {table_name}:\")\n",
    "            for r in validation_result.results:\n",
    "                if not r.success:\n",
    "                    print(f\"    - {r.expectation_config.expectation_type}\")\n",
    "    \n",
    "    print(\"\\n--- Overall Summary ---\")\n",
    "    success_rate = (passed_checks / total_checks) * 100 if total_checks > 0 else 0\n",
    "    overall_status = \"✅ PASS\" if failed_checks == 0 else \"❌ FAIL\"\n",
    "    \n",
    "    print(f\"Overall Status: {overall_status}\")\n",
    "    print(f\"Total Checks: {total_checks}\")\n",
    "    print(f\"Passed: {passed_checks}\")\n",
    "    print(f\"Failed: {failed_checks}\")\n",
    "    print(f\"Success Rate: {success_rate:.1f}%\")\n",
    "    \n",
    "    # Export results for pipeline\n",
    "    export_results_for_pipeline(results, overall_status, success_rate)\n",
    "    \n",
    "    return overall_status == \"✅ PASS\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def export_results_for_pipeline(results, overall_status, success_rate):\n",
    "    \"\"\"Export results in format consumable by Azure Pipelines\"\"\"\n",
    "    \n",
    "    # Create JUnit XML format for Azure Pipelines\n",
    "    junit_xml = f\"\"\"<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n",
    "<testsuite name=\"DataQualityValidation\" tests=\"{len(results)}\" failures=\"{len([r for r in results if not all(res.success for res in r['result'].results)])}\" time=\"0\">\n",\n",
    "\"\"\"\n",
    "    \n",
    "    for result in results:\n",
    "        table_name = result['table']\n",
    "        validation_result = result['result']\n",
    "        \n",
    "        passed = all(r.success for r in validation_result.results)\n",
    "        \n",
    "        if passed:\n",
    "            junit_xml += f'  <testcase classname=\"DataQuality\" name=\"{table_name}\" time=\"0\"/>\\n'\n",
    "        else:\n",
    "            junit_xml += f'  <testcase classname=\"DataQuality\" name=\"{table_name}\" time=\"0\">\\n'\n",
    "            junit_xml += f'    <failure message=\"Data quality checks failed for {table_name}\">\\n'\n",
    "            for r in validation_result.results:\n",
    "                if not r.success:\n",
    "                    junit_xml += f'      {r.expectation_config.expectation_type}: {r.result}\\n'\n",
    "            junit_xml += '    </failure>\\n'\n",
    "            junit_xml += '  </testcase>\\n'\n",
    "    \n",
    "    junit_xml += '</testsuite>'\n",
    "    \n",
    "    # Write JUnit XML file\n",
    "    with open('/tmp/data_quality_results.xml', 'w') as f:\n",
    "        f.write(junit_xml)\n",
    "    \n",
    "    # Write summary JSON\n",
    "    summary = {\n",
    "        'timestamp': datetime.now().isoformat(),\n",
    "        'workspace': WORKSPACE_NAME,\n",
    "        'lakehouse': LAKEHOUSE_NAME,\n",
    "        'overall_status': overall_status,\n",
    "        'success_rate': success_rate,\n",
    "        'total_tables': len(results),\n",
    "        'results': [{\n",
    "            'table': r['table'],\n",
    "            'passed': all(res.success for res in r['result'].results),\n",
    "            'total_checks': len(r['result'].results),\n",
    "            'failed_checks': len([res for res in r['result'].results if not res.success])\n",
    "        } for r in results]\n",
    "    }\n",
    "    \n",
    "    with open('/tmp/data_quality_summary.json', 'w') as f:\n",
    "        json.dump(summary, f, indent=2)\n",
    "    \n",
    "    print(\"Results exported for pipeline consumption\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Execute data quality validation\n",
    "if __name__ == \"__main__\":\n",
    "    try:\n",
    "        results = run_all_data_quality_checks()\n",
    "        \n",
    "        # Exit with error code if any checks failed\n",
    "        failed_tables = [r for r in results if not all(res.success for res in r['result'].results)]\n",
    "        \n",
    "        if failed_tables:\n",
    "            print(f\"\\nData quality validation FAILED for {len(failed_tables)} tables\")\n",
    "            exit(1)\n",
    "        else:\n",
    "            print(\"\\nAll data quality validations PASSED ✅\")\n",
    "            exit(0)\n",
    "            \n",
    "    except Exception as e:\n",
    "        print(f\"Critical error during validation: {str(e)}\")\n",
    "        exit(1)"
   ]
  }
